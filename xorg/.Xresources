!#include "/home/riddle/.Xresources.d/solarized"
!
! --  Extensions -- !
!URxvt.perl-ext-common:     matcher,keyboard-select,url-select,clipboard
!URxvt.keysym.M-Escape:     perl:keyboard-select:activate
!URxvt.keysym.M-u:          perl:url-select:select_next
!URxvt.keysym.M-s:          perl:keyboard-select:search
!URxvt.clipboard.autocopy:  true
!URxvt.keysym.M-c:          perl:clipboard:copy
!URxvt.keysym.M-v:          perl:clipboard:paste
!URxvt.keysym.M-C-v:        perl:clipboard:paste_escaped
!URxvt.url-select.launcher: /home/jason/Scripts/run_browser
!URxvt.matcher.rend.0:      -Uline fg16
!URxvt.matcher.button:      1


! -- Xterm settings -- !
xterm*termName: xterm-256color
xterm*locale: true
xterm*cursorColor: #395573
xterm*toolBar: false
xterm*scrollBar: false
xterm*rightScrollBar: false
xterm*background: #1A1A1A
xterm*foreground: #999999
xterm*faceName: anonymous pro:bold:size=10
!xterm*faceName: dejavu sans mono:medium:size=8.5
xterm*allowBoldFonts: false
xterm*geometry: 120x28


! -- Xscreensaver settings -- !
xscreensaver.mode: blank


