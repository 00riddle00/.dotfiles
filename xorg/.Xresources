
! Urxvt
URxvt.url-select.underline: true
URxvt.urlLauncher: firefox
URxvt.cursorUnderline: false
URxvt.cursorBlink: true
URxvt.geometry: 110x50
URxvt.scrollBar: false
URxvt*scrollBar_right: false
URxvt.borderLess: false
URxvt.borderColor: #37454e
URxvt.transparent: 0
URxvt*.shading: 0
URxvt*iconFile: /usr/share/icons/la-capitaine-icon-theme/apps/scalable/terminal.svg


URxvt.letterSpace: -1
!URxvt*urgentOnBell: true
URxvt.lineSpace: 2

! -- Fonts -- !
!URxvt.iconFile: /usr/share/icons/hicolor/scalable/apps/terminator.svg
!URxvt*font: xft:Source Code Pro Medium:size=7,xft:Inconsolata\ for\ Powerline:pixelsize=12
!URxvt*boldFont: xft:Source Code Pro Medium:size=7,xft:Inconsolata\ for\ Powerline:pixelsize=12
URxvt.font:     xft:bitstream vera sans mono:size=8:antialias=true
URxvt.boldFont: xft:bitstream vera sans mono:bold:size=8:antialias=true
!URxvt*font:xft:dejavu sans mono:medium:size=10
!URxvt*boldFont:xft:dejavu sans mono:medium:size=10
!URxvt.font: -*-terminus-medium-*-*-*-*-140-*-*-*-*-iso8859-1
!URxvt.boldFont: -*-terminus-medium-*-*-*-*-140-*-*-*-*-iso8859-1
!URxvt*font:xft:luxi mono:regular:pixelsize=10
!URxvt*boldFont:xft:luxi mono:regular:pixelsize=10

! Xft Font Configuration 
Xft.dpi: 96
Xft.antialias: true 
Xft.rgba: rgb
Xft.hinting: true 
Xft.hintstyle: hinstlight 


! -- Colours -- !
! -------------------------------------------------
! author:   jason ryan - http://jasonwryan.com/
! -------------------------------------------------
!! jwr dark
URxvt.background: #080808
URxvt.foreground: #999999

!black
*color0:#333333
!*color0:#242424
*color8:#474747
!red
*color1:#8A2F58
*color9:#CF4F88
!green
*color2:#287373
*color10:#53A6A6
!yellow
*color3:#914E89
*color11:#BF85CC
!blue
*color4:#395573
*color12:#4779B3
!magenta
*color5:#5E468C
*color13:#7F62B3
!cyan
*color6:#2B7694
*color14:#58AEB1
!white
*color7:#899CA1
