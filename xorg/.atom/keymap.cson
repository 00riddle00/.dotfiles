'body, .platform-win32, atom-workspace, atom-text-editor.vim-mode:not(.insert-mode), atom-pane, atom-text-editor.vim-mode.insert-mode, atom-text-editor.vim-mode.replace-mode, .image-view, atom-text-editor:not([mini]), .markdown-preview, atom-text-editor, .platform-linux, atom-text-editor.vim-mode, .native-key-bindings, atom-text-editor.vim-mode:not(.insert-mode), .project-find, .find-and-replace, atom-text-editor.vim-mode.normal-mode, atom-text-editor.vim-mode.visual-mode, atom-text-editor.vim-mode.operator-pending-mode, .tree-view':
