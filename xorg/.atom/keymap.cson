  'alt-f1': 'editor:toggle-line-comments'
  'ctrl-c': 'core:copy'
  'ctrl-v': 'core:paste'
  'ctrl-j': 'fuzzy-finder:toggle-file-finder'
  'cmd-v': 'pane:split-down-and-copy-active-item'
