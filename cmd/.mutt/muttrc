## .mutt/gmail
set ssl_starttls=yes
set ssl_force_tls=yes
set hostname="gmail.com"
set imap_user = "tomasgiedraitis@gmail.com"
set imap_pass = "voh7Faes"
set from="tomasgiedraitis@gmail.com"
set realname="Tomas Giedraitis"
set folder = "imaps://imap.gmail.com/:993"
set spoolfile = "imaps://imap.gmail.com/INBOX"
set postponed="imaps://imap.gmail.com/[Gmail]/Drafts"
set header_cache = "~/.mutt/cache/headers"
set message_cachedir = "~/.mutt/cache/bodies"
set certificate_file = "~/.mutt/certificates"
set signature="$CANDY/email/tomasgiedraitis-signature"
set sig_on_top=yes
set smtp_url = "smtps://tomasgiedraitis@smtp.gmail.com:465/"
set smtp_pass = "voh7Faes"
set move = no
set imap_keepalive = 900

